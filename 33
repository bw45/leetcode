#########################one
class Solution:
    def search(self, nums: List[int], target: int) -> int:
        self.nums=nums
        self.target=target
        if self.target in self.nums:
            return self.nums.index(self.target)
        else:
            return -1
##########################two
class Solution:
    def search(self, nums: List[int], target: int) -> int:
        return nums.index(target) if target in nums else -1
