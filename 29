class Solution:
    def divide(self, dividend,divisor):
        self.dividend=dividend
        self.divisor=divisor
        if self.divisor==1:
            return self.dividend
        elif self.divisor==-1:
            return -self.dividend
        else:
            if self.dividend<0 or self.divisor<0:
                return self.dividend//self.divisor+1
            else:
                return self.dividend//self.divisor
