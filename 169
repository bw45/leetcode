class Solution:
    def majorityElement(self, nums: List[int]) -> int:
        return [i for i in set(nums) if nums.count(i) > int(len(nums)/2)][0]
        
class Solution:
    def majorityElement(self, nums: List[int]) -> int:
        return next((i for i in set(nums) if nums.count(i) > int(len(nums)/2)))
