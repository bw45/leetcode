class Solution:
    def isPalindrome(self, x: int) -> bool:
        self.x=str(x)
        for i in range(len(self.x)//2):
            if self.x[i]==self.x[-1-i]:
                pass
            else:
                return False
        return True

class Solution:
    def isPalindrome(self, x: int) -> bool:
        return True if str(x)[::-1]==str(x) else False
