class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        for i,j in enumerate(nums):
            b=target-j
            nums[i]='a'
            if b in nums:
                return i,nums.index(b)


if __name__=='__main__':
    A=Solution()
    A.twoSum([3,2,4],6)
